{
  "name": "gp-reserve",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf ./lib",
    "build": "cross-env NODE_ENV=production npm run build:app && npm run build:server",
    "build:app": "webpack --config=webpack.config.js --mode=production",
    "build:server": "babel ./server --out-dir ./lib/server --copy-files",
    "start": "cross-env NODE_ENV=production node ./lib/server/server.js",
    "start:dev": "cross-env NODE_ENV=development babel-node ./server/server.js",
    "test": "npm run check:lint && npm run test:unit",
    "pretest:unit": "rimraf ./tests/out/coverage ./tests/out/unit",
    "test:unit": "karma start",
    "precheck:lint": "rimraf ./tests/out/lint-report.html",
    "check:lint": "eslint -f html --ext .js --ext .jsx src/ -o tests/out/lint-report.html"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "classnames": "^2.2.3",
    "cross-env": "^3.1.3",
    "express": "^4.16.2",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.24.0",
    "prop-types": "^15.5.7",
    "ramda": "^0.24.1",
    "react": "^16.4.2",
    "react-datepicker": "^2.5.0",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.7.2",
    "redux-actions": "^2.6.4",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-module-resolver": "^2.3.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-nodejs6": "^1.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "css-loader": "^0.23.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.6.1",
    "fetch-mock": "^5.11.0",
    "file-loader": "^1.1.6",
    "jasmine-core": "^2.6.1",
    "jasmine-enzyme": "^3.1.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.1.1",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-html-reporter": "^0.2.7",
    "karma-jasmine": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.3",
    "less": "^2.7.3",
    "less-loader": "^4.1.0",
    "node-sass": "^4.9.2",
    "postcss-loader": "^2.1.6",
    "prettier": "^1.16.4",
    "redux-mock-store": "^1.4.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.0",
    "svg-react-loader": "^0.4.6",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zxfzhang/gp-reserve"
  }
}
